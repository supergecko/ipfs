<view class="box"><label class="{{['label',labelfocus,value?'labelfocus':'',disabled?'disabled':'']}}">{{msg}}</label><input class="{{['inputitem',value?'inval':'']}}" type="{{type}}" name="{{name}}" id="{{name}}" password="{{password}}" data-event-opts="{{[['focus',[['focus',['$event']]]],['blur',[['blur',['$event']]]],['input',[['input',['$event']]]]]}}" value="{{value}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view>